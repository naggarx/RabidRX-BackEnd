{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "python/diabetesPredict.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/diabetes-predict", "dest": "python/diabetesPredict.py" },
    { "src": "/(.*)", "dest": "app.js" }
  ]
}
